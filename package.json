{
  "name": "vue-template",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve --port 3000",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:ci": "cypress run --headless",
    "cypress:ci:serve": "start-server-and-test serve 3000 cypress:ci"
  },
  "dependencies": {
    "@mdi/font": "5.9.55",
    "core-js": "^3.8.3",
    "pinia": "^2.0.13",
    "roboto-fontface": "*",
    "vue": "^3.2.13",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.3",
    "vuetify": "npm:@vuetify/nightly@next",
    "webfontloader": "^1.0.0",
    "js-cookie": "^2.2.1"
  },
  "devDependencies": {
    "cypress": "^9.6.0",
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "msw": "^0.39.2",
    "sass": "^1.38.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-vuetify": "~2.4.8",
    "vuetify-loader": "^2.0.0-alpha.0",
    "cross-env": "^7.0.3",
    "start-server-and-test": "^1.14.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "eqeqeq": "off",
      "no-unused-vars": "off",
      "no-debugger": "off",
      "no-mixed-spaces-and-tabs": "off",
      "no-unreachable": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": "14",
    "npm": "6"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
